#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/c/Users/USER/node/jwt/node_modules/.registry.npmjs.org/prebuild-install/5.2.4/node_modules/prebuild-install/bin.js/node_modules:/mnt/c/Users/USER/node/jwt/node_modules/.registry.npmjs.org/prebuild-install/5.2.4/node_modules/prebuild-install/node_modules:/mnt/c/Users/USER/node/jwt/node_modules/.registry.npmjs.org/prebuild-install/5.2.4/node_modules:/mnt/c/Users/USER/node/jwt/node_modules/.registry.npmjs.org/prebuild-install/node_modules:/mnt/c/Users/USER/node/jwt/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Users/USER/node/jwt/node_modules:/mnt/c/Users/USER/node/node_modules:/mnt/c/Users/USER/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../prebuild-install/5.2.4/node_modules/prebuild-install/bin.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/c/Users/USER/node/jwt/node_modules/.registry.npmjs.org/prebuild-install/5.2.4/node_modules/prebuild-install/bin.js/node_modules:/mnt/c/Users/USER/node/jwt/node_modules/.registry.npmjs.org/prebuild-install/5.2.4/node_modules/prebuild-install/node_modules:/mnt/c/Users/USER/node/jwt/node_modules/.registry.npmjs.org/prebuild-install/5.2.4/node_modules:/mnt/c/Users/USER/node/jwt/node_modules/.registry.npmjs.org/prebuild-install/node_modules:/mnt/c/Users/USER/node/jwt/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Users/USER/node/jwt/node_modules:/mnt/c/Users/USER/node/node_modules:/mnt/c/Users/USER/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/node_modules:/node_modules" node  "$basedir/../../../../../../prebuild-install/5.2.4/node_modules/prebuild-install/bin.js" "$@"
  ret=$?
fi
exit $ret
