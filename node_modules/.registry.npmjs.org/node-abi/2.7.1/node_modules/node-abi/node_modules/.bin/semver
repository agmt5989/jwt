#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/c/Users/USER/node/jwt/node_modules/.registry.npmjs.org/semver/5.6.0/node_modules/semver/bin/semver/node_modules:/mnt/c/Users/USER/node/jwt/node_modules/.registry.npmjs.org/semver/5.6.0/node_modules/semver/bin/node_modules:/mnt/c/Users/USER/node/jwt/node_modules/.registry.npmjs.org/semver/5.6.0/node_modules/semver/node_modules:/mnt/c/Users/USER/node/jwt/node_modules/.registry.npmjs.org/semver/5.6.0/node_modules:/mnt/c/Users/USER/node/jwt/node_modules/.registry.npmjs.org/semver/node_modules:/mnt/c/Users/USER/node/jwt/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Users/USER/node/jwt/node_modules:/mnt/c/Users/USER/node/node_modules:/mnt/c/Users/USER/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../semver/5.6.0/node_modules/semver/bin/semver" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/c/Users/USER/node/jwt/node_modules/.registry.npmjs.org/semver/5.6.0/node_modules/semver/bin/semver/node_modules:/mnt/c/Users/USER/node/jwt/node_modules/.registry.npmjs.org/semver/5.6.0/node_modules/semver/bin/node_modules:/mnt/c/Users/USER/node/jwt/node_modules/.registry.npmjs.org/semver/5.6.0/node_modules/semver/node_modules:/mnt/c/Users/USER/node/jwt/node_modules/.registry.npmjs.org/semver/5.6.0/node_modules:/mnt/c/Users/USER/node/jwt/node_modules/.registry.npmjs.org/semver/node_modules:/mnt/c/Users/USER/node/jwt/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Users/USER/node/jwt/node_modules:/mnt/c/Users/USER/node/node_modules:/mnt/c/Users/USER/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/node_modules:/node_modules" node  "$basedir/../../../../../../semver/5.6.0/node_modules/semver/bin/semver" "$@"
  ret=$?
fi
exit $ret
