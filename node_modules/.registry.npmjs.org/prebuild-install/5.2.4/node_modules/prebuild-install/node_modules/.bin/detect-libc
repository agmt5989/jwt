#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/c/Users/USER/node/jwt/node_modules/.registry.npmjs.org/detect-libc/1.0.3/node_modules/detect-libc/bin/detect-libc.js/node_modules:/mnt/c/Users/USER/node/jwt/node_modules/.registry.npmjs.org/detect-libc/1.0.3/node_modules/detect-libc/bin/node_modules:/mnt/c/Users/USER/node/jwt/node_modules/.registry.npmjs.org/detect-libc/1.0.3/node_modules/detect-libc/node_modules:/mnt/c/Users/USER/node/jwt/node_modules/.registry.npmjs.org/detect-libc/1.0.3/node_modules:/mnt/c/Users/USER/node/jwt/node_modules/.registry.npmjs.org/detect-libc/node_modules:/mnt/c/Users/USER/node/jwt/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Users/USER/node/jwt/node_modules:/mnt/c/Users/USER/node/node_modules:/mnt/c/Users/USER/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../detect-libc/1.0.3/node_modules/detect-libc/bin/detect-libc.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/c/Users/USER/node/jwt/node_modules/.registry.npmjs.org/detect-libc/1.0.3/node_modules/detect-libc/bin/detect-libc.js/node_modules:/mnt/c/Users/USER/node/jwt/node_modules/.registry.npmjs.org/detect-libc/1.0.3/node_modules/detect-libc/bin/node_modules:/mnt/c/Users/USER/node/jwt/node_modules/.registry.npmjs.org/detect-libc/1.0.3/node_modules/detect-libc/node_modules:/mnt/c/Users/USER/node/jwt/node_modules/.registry.npmjs.org/detect-libc/1.0.3/node_modules:/mnt/c/Users/USER/node/jwt/node_modules/.registry.npmjs.org/detect-libc/node_modules:/mnt/c/Users/USER/node/jwt/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Users/USER/node/jwt/node_modules:/mnt/c/Users/USER/node/node_modules:/mnt/c/Users/USER/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/node_modules:/node_modules" node  "$basedir/../../../../../../detect-libc/1.0.3/node_modules/detect-libc/bin/detect-libc.js" "$@"
  ret=$?
fi
exit $ret
